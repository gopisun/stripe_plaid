<dmx-serverconnect id="sc_l1" url="/api/USageOps/getSettLevel1"></dmx-serverconnect>
<script is="dmx-flow" id="flow_getLevel2" type="text/dmx-flow" src="/app/flows/getLevel2.json" dmx-param:agr_id="tableRepeat2[0].sett_int1_agreement_id" dmx-param:sett_term="tableRepeat2[0].sett_int1_settlement_term"></script>
<meta name="ac:route" content="/Settlements">
<dmx-serverconnect id="serverconnect_levelOne" url="/api/USageOps/getSettLevel1"></dmx-serverconnect>
<dmx-serverconnect id="serverconnect_levelTwo" url="/api/USageOps/getSettLevel2_filter" dmx-param:agr_id="tableRepeat2[0].sett_int1_agreement_id" dmx-param:sett_term="tableRepeat2[0].sett_int1_settlement_term"></dmx-serverconnect>
<div class="container mt-2 mb-2">
    <div class="row">
        <div class="col">
            <div class="table-responsive">
                <table class="table table-collapse table-bordered border-primary">
                    <thead class="style7 table-secondary">
                        <tr>
                            <th scope="row"></th>
                            <th>Sett int1 agreement</th>
                            <th>Sett int1 settlement term</th>
                            <th>Sett int1 sett qty</th>
                            <th>Sett int1 settlement amt</th>
                            <th>Agreement name</th>
                        </tr>
                    </thead>
                    <tbody is="dmx-repeat" dmx-generator="bs5table" dmx-bind:repeat="serverconnect_levelOne.data.api.data" id="tableRepeat_level1">
                        <tr>
                            <td><button id="btn1_showLevel2" class="btn" wappler-empty="Editable" wappler-command="editContent" data-bs-toggle="collapse" dmx-bind:data-bs-target="'#collapseLevel2'+$index"><i class="fas fa-angle-right"></i></button></td>
                            <td dmx-text="sett_int1_agreement_id"></td>
                            <td dmx-text="sett_int1_settlement_term"></td>
                            <td dmx-text="sett_int1_sett_qty"></td>
                            <td dmx-text="sett_int1_settlement_amt"></td>
                            <td dmx-text="agreementName"></td>
                        </tr>
                        <tr class="collapse mark" dmx-bind:id="'collapseLevel2'+$index">
                            <td></td>
                            <td colspan="9">
                                <div class="container-fluid">
                                    <div class="table-responsive">
                                        <table class="table table-collapse table-bordered table-sm border-success">
                                            <thead>
                                                <tr>
                                                    <th>Agreement name</th>
                                                    <th>Sett int1 settlement term</th>
                                                    <th>H tadig</th>
                                                    <th>V tadig</th>
                                                    <th>Sett int1 sett qty</th>
                                                    <th>Sett int1 settlement amt</th>



                                                </tr>
                                            </thead>
                                            <tbody is="dmx-repeat" dmx-generator="bs5table" dmx-bind:repeat="level2" id="tableRepeat2">
                                                <tr>
                                                    <td dmx-text="agreementName"></td>
                                                    <td dmx-text="sett_int1_settlement_term"></td>
                                                    <td dmx-text="h_tadig.tadig"></td>
                                                    <td dmx-text="v_tadig.tadig"></td>
                                                    <td dmx-text="sett_int1_sett_qty"></td>
                                                    <td dmx-text="sett_int1_settlement_amt"></td>



                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

